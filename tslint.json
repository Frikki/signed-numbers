{
  "extends": [
    "@motorcycle/tslint"
  ],
  "rules": {
    "unified-signatures": false,
    "readonly-interface": false,
    "no-unused-variable": [
      false
    ],
    "variable-name": [
      false
    ],
    "max-line-length": [
      true,
      200
    ],
    "no-param-reassign": false,
    "multiline-arrow": false,
    "no-invalid-this": [
      false
    ]
  }
}
